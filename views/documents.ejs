<!-- documents.ejs -->

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Documentos</title>
</head>
<body>
<h1>Lista de Documentos</h1>
<ul>
    <% documents.forEach(document => { %>
        <li>
            <a href="/documents/<%= document._id %>/download"><%= document.filename %></a>
        </li>
    <% }); %>
</ul>
</body>
</html>
